`ifdef RTL
	`timescale 1ns/10ps
	`include "MS.v"  
	`define CYCLE_TIME 15.0  
	`define DELAY 0
`endif
`ifdef GATE
	`timescale 1ns/1ps
	`include "MS_SYN.v"
	`define CYCLE_TIME 60.0
	`define DELAY 0
`endif

module PATTERN(
	rst_n, 
	clk, 
	maze ,
	in_valid ,
	mode ,
	out_valid,
	maze_not_valid,
	out_x, 
	out_y
);


//Port Declaration
output reg rst_n, clk, maze ,in_valid , mode ;
input  out_valid;
input  maze_not_valid;
input  [3:0]out_x, out_y ;

//-----------------  parameter --------------------
parameter CYCLE = `CYCLE_TIME;

//-------------------- wire & reg ---------------------


//------------------- integer or float --------------
integer PATNUM,total_latency,lat;
integer patcount;
integer k,i,j;
integer ans , your_ans;
integer out_valid_check_cnt ;
integer input_file, ans_file;
integer check_i;

always#(CYCLE/2.0) clk = ~clk;
initial clk = 0;


// --------------  output powerleakage check -----------------
initial begin
    forever @(negedge clk) out_sum_low_check;
end
//--------------   output valid check ---------------
/*
initial begin
    forever out_valid_cycle_check;
end
*/



initial begin

//please change the path to yourcurrently working directory 
input_file=$fopen("/misc/RAID2/COURSE/iclab/iclab30/Mid/EXERCISE/00_TESTBED/maze.txt","r");
ans_file=$fopen("/misc/RAID2/COURSE/iclab/iclab30/Mid/EXERCISE/00_TESTBED/sol.txt","r");
k = $fscanf(input_file,"size:%d",PATNUM);		

rst_n = 1;    
in_valid = 1'b0; 
force clk = 0;
total_latency = 0; 
reset_signal_task;

for(patcount=0;patcount<PATNUM;patcount=patcount+1) begin
    input_task;
    wait_OUT_VALID;
    check_ans;
end

YOU_PASS_task;

end

task check_ans;begin
    integer num_output;
    integer cn_i;
    integer ans_x,ans_y;
    k = $fscanf(ans_file,"num:%d",num_output);
    for (cn_i = 0 ; cn_i < num_output ; cn_i = cn_i +1)begin 
        k = $fscanf(ans_file,"%d,%d",ans_x,ans_y);
        @(negedge clk);
            if(out_x != ans_x)||(out_y != ans_y)begin
	        	$display ("------------------------------------------------");
	        	$display ("             Wrong Answer  No.%d             ",patcount);
	        	$display (" Expected: %d,%d                             ",ans_x,ans_y);
	        	$display (" Your    : %d,%d                               ",out_x,out_y);
	        	$display ("                                                ");
	        	$display ("------------------------------------------------");
	        	$finish;
            end
	    $display (" Correct         : %d,%d                               ",out_x,out_y);
    end
end endtask


task reset_signal_task; begin 
    #(0.5);   rst_n=0;
	#(2.0);
	if((out_valid !== 0)||(out_x !== 4'b0) || (out_y !== 4'b0)||(maze_not_valid !== 1'b0 ) ) begin
		$display ("------------------------------------------------");
		$display ("                  RESET VIOLATION               ");
		$display ("Output signal should be 0 after initial RESET   ");
		$display ("------------------------------------------------");
		$finish;
	end
	#(10);   rst_n=1;
	#(3);   release clk;
end endtask



task input_task; begin
	repeat(2)@(negedge clk);
	in_valid = 1;
	for(j=0;j<255;j=j+1) begin	
		k = $fscanf(input_file,"%d",maze);		
        if(j % 15 == 14)
            $display(maze)
        else
            $write(maze);
		@(negedge clk);	
	end   
	in_valid = 0;
	maze = 'dx;
end endtask

task wait_OUT_VALID; begin
  lat = -1;
  while(!out_valid) begin
	lat = lat + 1;
	if(lat == 100) begin
		$display ("-----------------------------------------");
		$display ("              FAIL!                      ");
	    $display ("          PATTERN NO.%4d             ",patcount);
		$display ("The execution latency are over 100 cycles");
		$display ("-----------------------------------------");
		repeat(2)@(negedge clk);
		$finish;
	end
	@(negedge clk);
  end
  total_latency = total_latency + lat;
end endtask

task out_sum_low_check;begin
    if(out_valid === 1'b0 && out_y && out_x  && maze_not_valid )begin
                $display("************************************************************");
                $display("*                    Power Leackage !                      *");
                $display("************************************************************");
                $finish;
    end
end endtask


task out_valid_cycle_check;begin
    out_valid_check_cnt = -1;
    @(posedge out_valid);
    while(out_valid == 'b1)begin
        out_valid_check_cnt = out_valid_check_cnt +1;
        if(out_valid_check_cnt > 6)begin
                    $display("************************************************************");
                    $display("*                    outvalid should be 6 cycle !          *");
                    $display("************************************************************");
                    $finish;
        end
        @(negedge clk);
    end

end endtask


